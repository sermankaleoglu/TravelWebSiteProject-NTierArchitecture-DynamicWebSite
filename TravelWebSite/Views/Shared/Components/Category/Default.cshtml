@model List<Category>
<div class="list-group">
    @foreach (var Category in Model)
    {
        <a href="http://localhost:5263/product/list/@Category.CategoryId" class="list-group-item list-group-item-action @(ViewBag.SelectedCategory==Category.CategoryId.ToString()?" active":" ")">@Category.Name</a>    
    }
</div>